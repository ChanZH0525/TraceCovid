            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InfoPageTwo"
            android:exported="false" />
        <activity
            android:name=".InfoPageOne"
            android:exported="false" />
        <activity
            android:name=".PwdUpdatedPage"
            android:exported="false" />
        <activity
            android:name=".StarterPage"
            android:exported="false" />
        <activity
            android:name=".FAQ"
            android:exported="false" />
        <activity
            android:name=".CheckInHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".SelfReport"
            android:exported="false" />
        <activity
            android:name=".RiskAssessment"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>